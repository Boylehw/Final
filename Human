public class Human extends Biped {

  String Name;
	int Age;
	int Weight; // lbs
	int Speed; // mph
	int Acceleration; // out of 5
	int RunDistance; // miles?
	String Talk;

	public String getName() {
		Name = "Hulk";
		return Name;
	}

	public String getTalk() {
		Talk = "Hello";
		return Talk;
	}

	public int getAge() {
		Age = 35;
		return Age;
	}

	public int getWeight() {
		Weight = 220;
		return Weight;
	}

	public int getSpeed() {
		Speed = 7;
		return Speed;
	}

	public int getAcceleration() {
		Acceleration = 3;
		return Acceleration;
	}

	public int getRunDistance() {
		RunDistance = 6;
		return RunDistance;
	}

	@Override
	public String toString() {
		return String.format(
				"%s %s%s\n%s %s\n%s: %s\n%s: %s\n%s: %s\n%s: %s\n%s: %s",
				"My name is", getName()," and I am human", "I can say", getTalk(), "My age is",
				getAge(), "I weigh", getWeight(), "My speed is", getSpeed(),
				"I can accelerate at a speed of", getAcceleration(),
				"I can run", getRunDistance());
	}

}
